{
  "name": "@imagiro/stream-registry",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "clean": "tsc --build --clean",
    "lint": "eslint .",
    "test": "jest --passWithNoTests"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
