  },
  "dependencies": {
    "@imagiro/stream-bip122": "workspace:*",
    "@imagiro/stream-core": "workspace:*",
    "@imagiro/stream-eip155": "workspace:*",
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@workspace/jest-preset": "workspace:*",
    "@workspace/tsconfig": "workspace:*"
  }
}
